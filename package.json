{
  "name": "@miracl/mfa-client-js",
  "version": "1.2.0",
  "description": "Headless MFA client library",
  "main": "index.js",
  "config": {
    "cryptoPath": "node_modules/@miracl/milagro-crypto-js/src/",
    "cryptoSrc": "DBIG.js BIG.js FP.js ROM.js UInt64.js HASH256.js HASH384.js HASH512.js RAND.js AES.js GCM.js ECP.js FP4.js FP12.js PAIR.js MPIN.js MPINAuth.js",
    "cryptoTmp": "../../../../.tmp/crypto.js",
    "cryptoTmpFile": ".tmp/crypto.js",
    "libMfa": "lib/mfa.js",
    "buildOut": "dist/mfa.js"
  },
  "scripts": {
    "step1": "rm -rf dist .tmp",
    "step2": "mkdir .tmp dist",
    "step3": "cd $npm_package_config_cryptoPath && cat $npm_package_config_cryptoSrc > $npm_package_config_cryptoTmp",
    "step4": "cat $npm_package_config_cryptoTmpFile $npm_package_config_libMfa > $npm_package_config_buildOut",
    "build": "npm run step1 && npm run step2 && npm run step3 && npm run step4",
    "test": "mocha",
    "cover": "istanbul cover _mocha",
    "lcov": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miracl/mfa-client-js.git"
  },
  "keywords": [
    "JS",
    "MFA",
    "Headless",
    "Library"
  ],
  "author": "MIRACL",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miracl/mfa-client-js/issues"
  },
  "homepage": "https://github.com/miracl/mfa-client-js#readme",
  "dependencies": {
    "@miracl/milagro-crypto-js": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "node-localstorage": "^1.3.0",
    "sinon": "^2.1.0"
  }
}
